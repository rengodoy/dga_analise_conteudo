@article{ WOS:001233363800001,
Author = {Medina-Perez, Miguel and Guzman, Giovanni and Saldana-Perez, Magdalena
   and Legaria-Santiago, Valeria Karina},
Title = {Medical Support Vehicle Location and Deployment at Mass Casualty
   Incidents},
