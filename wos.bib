@article{ WOS:000672618700011,
Author = {Paulo, Joelson de Souza and Gama, Janyluce Rezend and Caliman, Douglas
   Roriz},
Title = {GOVERNMENTAL OPEN DATA MANAGEMENT: FROM MODEL TO PRACTICE},
