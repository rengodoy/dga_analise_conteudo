@article{ WOS:001342809300001,
Author = {Vidal-Cabo, Christian and Sanchez-Perez, Enrique Alfonso and
   Ferrer-Sapena, Antonia},
Title = {Perception and Reuse of Open Data in the Spanish University Teaching and
   Research Community},
Journal = {DATA},
Year = {2024},
