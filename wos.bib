@article{ WOS:000511068200003,
Author = {Schrotter, Gerhard and Huerzeler, Christian},
Title = {The Digital Twin of the City of Zurich for Urban Planning},
Journal = {PFG-JOURNAL OF PHOTOGRAMMETRY REMOTE SENSING AND GEOINFORMATION SCIENCE},
