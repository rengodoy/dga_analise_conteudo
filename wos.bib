@article{ WOS:000649155700001,
Author = {Gao, Yingying and Janssen, Marijn and Zhang, Congcong},
Title = {Understanding the evolution of open government data research: towards
   open data sustainability and smartness},
Journal = {INTERNATIONAL REVIEW OF ADMINISTRATIVE SCIENCES},
