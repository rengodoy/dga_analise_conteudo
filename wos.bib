@article{ WOS:000860990800010,
Author = {Braga, Juliana Vasconcelos and Ribeiro Gouveia, Feliz Alberto},
Title = {Brazilian Initiatives to Linked Open Government Data: An Analysis of the
   Level of Openness of Data in Brazilian Government Platforms},
