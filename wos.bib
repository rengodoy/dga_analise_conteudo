Title = {Characteristics of Clinics without National Health Insurance Contracts:
   A Nationwide Survey in Taiwan},
Journal = {INTERNATIONAL JOURNAL OF ENVIRONMENTAL RESEARCH AND PUBLIC HEALTH},
