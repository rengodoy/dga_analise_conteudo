@article{ WOS:001252462800001,
Author = {Aziz, Abdul and Jose Herrera-Murillo, Dagoberto and Nogueras-Iso, Javier
   and Lacasta, Javier and Lopez-Pellicer, Francisco J.},
Title = {Identifying the Evolution of Open Government Data Initiatives and Their
   User Engagement},
