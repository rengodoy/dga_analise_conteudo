@article{ WOS:001075488000003,
Author = {Albano, Claudio Sonaglio and da Silva Craveiro, Gisele and Carossi,
   Daniel Fernando},
Title = {Supply and demand of open government data: a study in brazilian federal
   universities},
Journal = {REVISTA DE GESTAO E SECRETARIADO-GESEC},
Year = {2023},
Volume = {14},
Number = {9},
Pages = {15191-15213},
Abstract = {Governments have invested in open data processes as one of the ways to
   improve management and promote greater transparency in society. In this
   sense, reconciling the data offered with the effective interest of
   society in them is essential. The objective of this work was to verify
   whether the data that society requests are consistent with the
   information made available by federal universities, in an open format.
   By accessing secondary data (open data plans and federal university
   pages), between 2016 and 2021, we can highlight the following results:
