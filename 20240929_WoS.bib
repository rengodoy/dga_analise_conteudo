@article{ WOS:000600153700001,
Author = {Ajana, Btihaj},
Title = {Personal metrics: Users' experiences and perceptions of self-tracking
   practices and data},
Journal = {SOCIAL SCIENCE INFORMATION SUR LES SCIENCES SOCIALES},
