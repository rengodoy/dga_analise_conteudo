@article{ WOS:001057026500001,
Author = {Abbas, Syed Wasim and Hamid, Muhammad and Alkanhel, Reem and Abdallah,
   Hanaa A.},
Title = {Official Statistics and Big Data Processing with Artificial
   Intelligence: Capacity Indicators for Public Sector Organizations},
