@article{ WOS:000530993700014,
Author = {Morais, Palloma Damascena and da Costa, Tarcisio Valerio and Seabra
   Filho, Genival Quirino},
Title = {Characterization of solid waste from the city Sao Jose de Espinharas
   (PB)},
Journal = {REVISTA TECNOLOGIA E SOCIEDADE},
Year = {2020},
Volume = {16},
Number = {41},
Pages = {163-176},
Abstract = {The characteristics of urban solid waste (USW) vary between
   municipalities, thus, its characterization becomes of great importance
   for management planning by public administration. Therefore, the present
   study aimed to characterize the USW qualitatively and quantitatively in
   the municipality of Sao Jose de Espinharas - PB, aiming to contribute to
   the planning of its integrated management. The study was carried out
   with a total sampling of 100 residences, using the method of quartering,
   which consists of the process of dividing into four equal parts a
   pre-homogenized sample. The study obtained from the total production of
   daily waste, 56.59\% organic matter, 27.96\% recyclable material and
   15.45\% waste. The value of solid waste per capita of 0.4139 kg per day
   was also obtained, with an estimate of 1.98 ton per day. The study
   contributed to environmental monitoring in the USW collection system.},
