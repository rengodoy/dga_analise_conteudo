@article{ WOS:000941202300001,
Author = {Qiu, Peiyao and Sorourkhah, Ali and Kausar, Nasreen and Cagin, Tonguc
   and Edalatpanah, Seyyed Ahmad},
Title = {Simplifying the Complexity in the Problem of Choosing the Best
   Private-Sector Partner},
