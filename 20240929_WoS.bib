@article{ WOS:001007665100001,
Author = {Yudiatmaja, Wayu Eko and Salomo, Roy Valiant and Prasojo, Eko},
Title = {Fostering Innovative Behavior of Millennial Public Employee Through
   Leadership Styles and Organizational Trust},
