@article{ WOS:001316599700021,
Author = {Lazor, Oleh and Lazor, Oksana and Yunyk, Iryna and Zubar, Ivan and
   Dzeveliuk, Maryna and Dzeveliuk, Andrii},
Title = {THE IMPACT OF E-GOVERNANCE ON SHORTENING THE PUBLIC SERVICE DELIVERY
   TERMS},
Journal = {TELOS-REVISTA DE ESTUDIOS INTERDISCIPLINARIOS EN CIENCIAS SOCIALES},
Year = {2024},
