@article{ WOS:000677909900028,
Author = {Kovalova, Svitlana and Koval, Alla and Panchenko, Snizhana and Pronina,
   Oksana and Bykov, Roman},
Title = {Features of Decentralization Processes of Developed Countries in the
   Post-Pandemic Society},
