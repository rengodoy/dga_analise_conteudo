@article{ WOS:000577502200008,
Author = {Strelchenko, Oksana and Okhrimenko, Svitlana and Pavlov, Dmytro},
Title = {The Role of the Institute of Medical Law in the Postmodern Society},
Journal = {POSTMODERN OPENINGS},
