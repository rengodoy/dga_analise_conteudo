@article{ WOS:000674299700002,
Author = {Zam, Ruhana Mohamad and Nawawi, Anuar and Salin, Ahmad Saiful Azlin
   Puteh},
Title = {Effectiveness of Auditor General's Online Dashboard Disclosures:
   Qualitative Perspectives from Malaysian Members of Parliament},
Journal = {PERTANIKA JOURNAL OF SOCIAL SCIENCE AND HUMANITIES},
Year = {2021},
Volume = {29},
Number = {2},
Pages = {755-770},
Month = {JUN},
Abstract = {The Malaysian Auditor General's Report is tabled in Parliament every
   year. This report covers issues from government ministries, departments,
   and agencies. Once the report is tabled, it is published to the public
   in hardcopy. Beyond a thick physical hardcopy, the emergence of
   information technology allows the public to browse, analyze, and review
   the report online via the Auditor General's Online Dashboard. However,
   the public and many other stakeholders, including Malaysian members of
   Parliament, are not aware of the existence of those facilities.
   Therefore, this research aims to gather information on the effectiveness
   of the Auditor General's Online Dashboard disclosure and whether such
   disclosures provide satisfactory information to users and particularly
   to members of Parliament. A qualitative survey of 30 members of
   Parliament through convenience sampling is used. This study found that
   lack of promotion contributed mostly to unawareness of the existence of
   the Auditor General's Online Dashboard disclosure.},
