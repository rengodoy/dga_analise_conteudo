@article{ WOS:000683585100006,
Author = {Manfredi, Simonetta and Clayton-Hathway, Kate and Cousens, Emily},
Title = {Increasing Gender Diversity in Higher Education Leadership: The Role of
   Executive Search Firms},
Journal = {SOCIAL SCIENCES-BASEL},
Year = {2019},
Volume = {8},
Number = {6},
Month = {JUN},
Abstract = {Women are under-represented in leadership roles in United Kingdom Higher
   Education Institutions (HEIs). Existing scholarship focuses on
   institutional barriers, which include cognitive bias and entrenched
   homosocial cultures, rather than external factors such as the use of
   executive search firms (ESFs) in recruitment and selection. Recent
   research indicates that the use of ESFs is increasing for senior HEI
   appointments. This analysis offers insights on these firms' involvement
   from a gender equality perspective, based on the results from a study
   that used a `virtuous circle' approach to research and knowledge
   exchange. The requirement for HEIs to pay `due regard' to equality
   considerations under the Public Sector Equality Duty provides a
   framework for analysis. This paper provides new insights on the dynamics
   within recruitment processes when ESFs are involved and on how a
   legislative approach can leverage better equality outcomes.},
