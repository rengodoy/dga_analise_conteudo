@article{ WOS:000920300200002,
Author = {Luksic, Igor},
Title = {POLITICAL SCIENCE IN THE REPUBLIC OF SLOVENIA: 60 YEARS OF
   INSTITUTIONALISATION},
Journal = {TEORIJA IN PRAKSA},
Year = {2022},
Volume = {59},
Number = {1},
Pages = {195-221},
Abstract = {Political science became a strong established scientific discipline in
   the world only in the twentieth century as a reflection and support for
   the processes of democratization and decolonization. With the
   consolidation of the project of building the Slovenian state within the
   framework of the project of Yugoslav socialist self-management and
   deepening of non-party democracy, the first institution for political
   sciences in Slovenia was also formed - the College of Political
   Sciences. At that time, political science included the social sciences
   as a whole, i.e. sociology, history, philosophy, psychology, economics,
   law. Political science gradually became a narrower term than social
   science, and political science became one of the social science
   disciplines. This is also shown by the name of the faculty: from the
   College of Political Sciences to the Faculty of Social Sciences. At the
   same time, a process of differentiation took place within the framework
   of political science, so that international relations, defense science,
   policy analysis, and public administration and political theory
   developed as independent fields in Slovenian conditions. These four
   fields have also formed special study programs at undergraduate,
   master's and doctoral levels. Research centers in the field of political
   science are also organized in this way.},
