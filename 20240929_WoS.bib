@article{ WOS:000726842100001,
Author = {Steenmans, Katrien and Taylor, Phillip and Steenmans, Ine},
Title = {Blockchain Technology for Governance of Plastic Waste Management: Where
   Are We?},
Journal = {SOCIAL SCIENCES-BASEL},
Year = {2021},
Volume = {10},
