@article{ WOS:000405225700001,
Author = {Kurata, Keiko and Matsubayashi, Mamiko and Mine, Shinji},
Title = {Identifying the Complex Position of Research Data and Data Sharing Among
   Researchers in Natural Science},
Journal = {SAGE OPEN},
Year = {2017},
Volume = {7},
Number = {3},
Month = {JUL 8},
Abstract = {This article aims to provide an overview of researchers' practices and
   perceptions on data use and sharing. Semistructured interviews were
   conducted with 23 Japanese researchers in the natural sciences to
   identify their research practices and data use, including data sharing.
   We divided the interview scripts into meaningful phrases as a unit of
   analysis. Next, we focused on 406 statements on research data and
   reanalyzed them based on four aspects: stance on research data,
   practices and perceptions of data use, range of data sharing, and data
   type. A cluster analysis identified 14 clusters, which were divided into
   five groups: open access for data, restricted access for data, data
   interpretation, data processing and preservation, and data
   infrastructure. Our results reveal the complexity and diversity of the
   relationship between data and research practices. That is, the practice
   of research data sharing is heterogeneous, with no one size fits all
   between and among researchers.},
