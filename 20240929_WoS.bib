@article{ WOS:000482231900009,
Author = {Zaimah, R.},
Title = {The probability factor influences the level of financial well-being of
   workers in Malaysia},
Journal = {GEOGRAFIA-MALAYSIAN JOURNAL OF SOCIETY \& SPACE},
