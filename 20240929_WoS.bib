@article{ WOS:000751672600011,
Author = {Lada, Oleksandr and Kotsur, Vitalii and Kotsur, Lesya and Redziuk,
   Viacheslav and Gylenko, Yegor},
Title = {The Relations between Soviet Government Entities and Scientific
   Institutions in the Context of a Postmodern Approach to History},
