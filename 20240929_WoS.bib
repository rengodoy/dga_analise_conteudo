@article{ WOS:000612547500004,
Author = {Klymenko, Ihor Volodymyrovych and Shvets, Dmytro Volodymyrovych and
   Tsyhanov, Oleh and Mohilevska, Liudmyla Hennadiivna},
Title = {Services Provided by Public Authorities: Features of Legal Regulation in
   Ukraine and the European Union},
