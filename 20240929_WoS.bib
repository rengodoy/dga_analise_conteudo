@article{ WOS:000791047400026,
Author = {Pyroha, Ihor and Tokarchuk, Liudmyla and Perezhniak, Boris and
   Nikitenko, Liliia and Berch, Veronika},
Title = {E-democracy: an urgent need or a tribute to fashion? Legal analysis},
Journal = {AMAZONIA INVESTIGA},
