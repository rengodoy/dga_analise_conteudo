@article{ WOS:000655021100015,
Author = {Rafajac, Ozren and Washington, Sara Shanice},
Title = {MULTICULTURAL COMMUNICATION STRATEGY},
Journal = {ZBORNIK VELEUCILISTA U RIJECI-JOURNAL OF THE POLYTECHNICS OF RIJEKA},
