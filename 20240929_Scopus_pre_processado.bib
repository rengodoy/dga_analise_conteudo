@article{martinez_gutierrez_reform_2023,
	title = {{REFORM} {FOR} {DATA} {PUBLIC} {ADMINISTRATION}; [{REFORMA} {PARA} {UNA} {ADMINISTRACIÓN} {DE} {DATOS}]},
	volume = {2023},
	issn = {18858252},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85180831516&doi=10.58992%2frcdp.i67.2023.4094&partnerID=40&md5=1c574636653d59a99acde75979218d8d},
