	author = {Pangemanan, Fanley Noldi and Dondokambey, Olly and Ratag, Semuel Paulus and Kimbal, Alfon and Paat, Frangky Jessy and Monintja, Donald K. and Waworundeng, Welly and Kalengkongan, Novia Wen-Wen},
	year = {2024},
