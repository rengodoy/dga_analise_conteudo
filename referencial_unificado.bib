@article{Tang202111,
    abbrev_source_title = {Data. Inf. Manag.},
    abstract = {In this paper, we report the results of a meta-analysis of 50 publications on international Open Government Data (OGD) practices instantiated via their OGD sites or portals. Specific information about 67 individual countries'/regions' OGD sites was extracted and compared, including the levels of OGDs, the number and types of data formats, the number of datasets, and the number and types of data categories. Upon comparing the data characteristics by types and countries, the top 10 countries/regions based on the number of data formats, datasets, and data categories were presented. Significant correlations were found among individual countries' number of data formats, datasets, and data categories. Follow-up research that examines, confirms, and traces the data processing capacity of international OGDs is currently underway. © 2021 Rong Tang et al., published by Sciendo},
