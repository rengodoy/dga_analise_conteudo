    author = {Sabri, Nor A. M. and Emran, Nurul A. and Harum, Norharyati},
    author_keywords = {Data Veracity; Open data policy; Open data principles; Open government data; Quality criteria},
    doi = {10.35940/ijitee.L2908.1081219},
