    correspondence_address = {B. Wilson; Urban and Environmental Planning, School of Architecture, University of Virginia, Charlottesville, 110 Bayly Dr, 22903, United States; email: bw6xs@virginia.edu},
    doi = {10.1016/j.tele.2020.101526},
    issn = {07365853},
    journal = {Telematics and Informatics},
    keywords = {Government data processing; Service industry; Telecommunication industry; Telephone sets; Digital inclusion; Government staff; Local government; Monitoring mechanisms; Public and private sector; Qualitative analysis; Semi-structured; User perspectives; Open Data},
    language = {English},
    note = {Cited by: 25; All Open Access, Hybrid Gold Open Access},
    publication_stage = {Final},
    publisher = {Elsevier Ltd},
    source = {Scopus},
