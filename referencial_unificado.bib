    publication_stage = {Final},
    publisher = {Wolters Kluwer Medknow Publications},
    source = {Scopus},
    title = {Forecasting Indian infant mortality rate: An application of autoregressive integrated moving average model},
    type = {Article},
    url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85081310589&doi=10.4103%2fjfcm.JFCM_51_18&partnerID=40&md5=a07bfb82810cf937fecab86d6115dc3e},
