    author_keywords = {Data collection; Health information systems; Open data; World health organization},
    correspondence_address = {C.J. Greenberg; Wenzhou-Kean University, Wenzhou, 88 Daxue Lu, Ouhai, China; email: cjgberg@wku.edu.cn},
