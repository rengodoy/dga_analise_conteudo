    author = {Alexopoulos, Charalampos and Saxena, Stuti and Janssen, Marijn and Rizun, Nina and Lnenicka, Martin and Matheus, Ricardo},
    author_keywords = {barrier; demand-side; Open Government Data; rootcause analysis; supply-side},
