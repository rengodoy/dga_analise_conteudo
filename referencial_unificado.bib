    doi = {10.5334/DSJ-2021-037},
    issn = {16831470},
    journal = {Data Science Journal},
    keywords = {Government data processing; Open Data; Activity-based; Design Principles; Information products; Innovation methods; Mixed method; Open datum; Product and services; Reuse; User information need; User involvement; Product design},
