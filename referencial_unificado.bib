    number = {5},
    pages = {364 – 368},
    publication_stage = {Final},
    publisher = {Routledge},
    source = {Scopus},
    title = {The (im)possibilities of open data?},
    type = {Article},
    url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85066848964&doi=10.1080%2f09540962.2019.1611240&partnerID=40&md5=5021282b5c001fa0f248f662924e707b},
