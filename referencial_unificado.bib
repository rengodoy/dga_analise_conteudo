    language = {English},
    note = {Cited by: 6; All Open Access, Gold Open Access},
    number = {2},
    pages = {329 – 348},
    publication_stage = {Final},
    publisher = {Statistics Sweden},
    source = {Scopus},
    title = {End user licence to open government data? A simulated penetration attack on two social survey datasets},
    type = {Article},
