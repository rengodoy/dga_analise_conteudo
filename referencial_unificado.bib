    doi = {10.3390/info15050260},
    issn = {20782489},
    journal = {Information (Switzerland)},
    keywords = {Decision making; Disaster prevention; Earthquakes; Emergency services; Location; Coverage problem; Large scale disasters; Location allocation problem; Mass casualty incidents; Maximum coverage; Maximum coverage problem; Multi-objectives optimization; P-median problems; Support vehicles; Vehicle location; Multiobjective optimization},
    language = {English},
